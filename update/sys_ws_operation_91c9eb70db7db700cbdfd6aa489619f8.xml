        <name>Create Remote Update Set Content</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {
	
	if (!request.getHeader('Authorization')) {
		response.setStatus(401);
		response.setContentType('/application/json');
		response.setBody({ 'error' : 'Authentication expired'});
	}
	
	var payload = request.body.data;
	var setID = request.pathParams.sys_id;
	var utils = new x_wms_pulse.PulseUtils();
	utils.setDebug(true); // Debugging
	var answer = utils.localInsertUpdateSetContent(setID, payload);
	
	if (answer.success) {
		response.setStatus(200);
	} else {
		response.setStatus(400);
	}
	response.setContentType('application/json');
	response.setBody(answer);

})(request, response);]]></operation_script>
