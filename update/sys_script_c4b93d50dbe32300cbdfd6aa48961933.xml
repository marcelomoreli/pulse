	g_scratchpad.curr_env_url = '';
	g_scratchpad.curr_us_sys_id = '';
	if(current.current_instance){
		//used in ui macro go_to_update_set
		if (current.update_set_json.indexOf('{') > -1) {
			var usObj = JSON.parse(current.update_set_json + '');
			g_scratchpad.curr_env_url = current.current_instance.url;
			g_scratchpad.curr_us_sys_id = usObj[current.current_instance.name].local;
		}
	}
})(current, previous);]]></script>
